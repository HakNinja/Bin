#include<stdio.h>

int main() {
      int first, second, temp;
      
      printf("Enter first number: ");
      scanf("%d", &first);
      printf("Enter second number: ");
      scanf("%d", &second);

      // Value of first is assigned to temp variable
      temp = first;

      // Value of second is assigned to first variable
      first = second;

      // Value of temp (initial value of first) is assigned to second variable
      second = temp;

      printf("\nAfter swapping : ");
      printf("\n\tfirstNumber = %d\n", first);
      printf("\tsecondNumber = %d", second);
      
      return 0;
}