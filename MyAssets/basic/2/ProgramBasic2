#include <stdio.h>

int main() {   
    int number;
   
    printf("Enter an integer : ");  
    
    // reads and stores input
    //      read integer(%d) and store at variable address
    scanf("%d", &number);

    // displays output
    //      %d for display value at respectively variable 
    printf("Entered integer : %d", number);
    
    return 0;
}