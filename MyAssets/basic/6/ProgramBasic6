#include <stdio.h>

int main() {  
    char c;

    printf("Enter a character: ");    
    //input any character
    scanf("%c", &c);  
    
    // %d displays the integer value (ASCII value) of a character
    // %c displays the actual character
    printf("\nASCII value of %c is %d", c, c);
    
    return 0;
}