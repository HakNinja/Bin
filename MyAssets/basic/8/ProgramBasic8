#include <stdio.h>

int main() {
    int dividend, divisor, quotient, remainder;
    
    printf("Enter dividend: ");
    scanf("%d", &dividend);
    printf("Enter divisor: ");
    scanf("%d", &divisor);

    // Computes quotient
    quotient = dividend / divisor;

    // Computes remainder
    remainder = dividend % divisor;

    //  print quotient and remainder
    printf("\nValue of quotient = %d\n", quotient);
    printf("Value of Remainder = %d", remainder);
    
    return 0;
}